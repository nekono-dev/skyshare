---
export const prerender = false
import { baseurl, pagesPrefix } from "@/env/envs"

const baseurlNormalized =
    baseurl.replace(/\/$/, "") + pagesPrefix.replace(/\/$/, "")

const { slug } = Astro.params
console.debug(
    `Redirecting legacy post slug: ${baseurlNormalized}/legacy/${slug}`,
)
return await Astro.rewrite(`${baseurlNormalized}/legacy/${slug}`)
---
